{
  "name": "news",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "copy1": "cp -f ./out/article.html ./out/article/",
    "rename": "mv -f ./out/article/article.html ./out/article/index.html",
    "copy2": "cp -pR ./out/article/* ../public/news/",
    "copy3": "cp -f ./out/index.html ../public/index.html",
    "export": "yarn copy1 && yarn rename && yarn copy2 && yarn copy3"
  },
  "dependencies": {
    "next": "10.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}
